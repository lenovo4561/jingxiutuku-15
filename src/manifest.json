{
  "package": "com.jinxiutkjsjx.brocadetk",
  "name": "锦绣图库",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.app"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.device"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug",
    "designWidth": 250
  },
  "router": {
    "entry": "pages/Splash",
    "pages": {
      "pages/Splash": {
        "component": "index"
      },
      "pages/Main": {
        "component": "index"
      },
      "pages/Demo": {
        "component": "index"
      },
      "pages/DemoDetail": {
        "component": "index"
      },
      "pages/Detail": {
        "component": "index"
      },
      "pages/AvatarDetail": {
        "component": "index"
      },
      "pages/EmojiDetail": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Splash": {
        "titleBar": false,
        "menu": false,
        "fullScreen": true
      },
      "pages/Main": {
        "titleBar": false,
        "menu": false
      },
      "pages/Demo": {
        "titleBarText": "快应用示例模版",
        "menu": false
      },
      "pages/DemoDetail": {
        "titleBarText": "详情页"
      },
      "pages/Detail": {
        "titleBar": false,
        "fullScreen": true,
        "menu": false
      },
      "pages/AvatarDetail": {
        "titleBar": false,
        "fullScreen": true,
        "menu": false
      },
      "pages/EmojiDetail": {
        "titleBar": false,
        "fullScreen": true,
        "menu": false
      }
    }
  },
  "deviceTypeList": ["phone"]
}
